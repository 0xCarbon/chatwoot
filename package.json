  "engines": {
    "node": "20.x",
    "npm": ">=6.x",
    "yarn": "1.22.x"
  },
  "husky": {
    "hooks": {
      "pre-push": "sh bin/validate_push"
    }
  },
  "lint-staged": {
    "app/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "scss-lint"
    ]
  }
}
